package day6;

import java.util.Arrays;

public class ArraySortPrg {

	public static void main(String[] args) {
		int ar[]= {11,2,3,1,4,5,6};
		Arrays.sort(ar);
		System.out.println(Arrays.toString(ar));
		
		User user[]=new User[4];
		
		user[0]=new User(10003,"Naveen kumar",45000);
		user[1]=new User(10004,"Arvind Kejriwal",30000);
		user[2]=new User(10005,"Narendra Modi",28000);
		user[3]=new User(10006,"Nitish Kumar",29000);
		
		Arrays.sort(user);
		System.out.println("After sorting:");
		
		for(User u:user)
		{
			System.err.println(u);
		}
	}

}
