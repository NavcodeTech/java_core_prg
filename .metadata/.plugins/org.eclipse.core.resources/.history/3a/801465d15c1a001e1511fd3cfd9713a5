package day2;

public class Circle extends Shape{
	@Override
	void area() {
		// TODO Auto-generated method stub
		System.out.println("area of circle with radius "+radius+" is "+Math.PI*radius*radius);
	}

	float radius;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Circle c1=new Circle();
		c1.radius=7.5f;
		c1.area();
	}

}
